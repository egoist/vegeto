  "description": "Simple micro templating with JavaScript expression support.",
  "repository": {
    "url": "egoist/vegeto",
    "type": "git"
  },
  "main": "dist/vegeto.common.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest && npm run lint",
